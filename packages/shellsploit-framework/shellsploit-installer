#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet https://github.com/vasco2016/shellsploit-framework ${HOME}/.local/share/shellsploit-framework
[[ -d ${HOME}/.local/share/shellsploit-framework ]] && {
  echo "Installing requirements ...";
  chmod 751 -R ${HOME}/.local/share/shellsploit-framework;
  python -m pip --no-color --quiet --no-cache-dir install capstone readline pefile colorama pylzma;
} || {
  echo -en "\e[31mE:\e[0m shellsploit-framework directory missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
