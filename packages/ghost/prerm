#!/data/data/com.termux/files/usr/bin/bash
v=$(python --version|grep -Eo "[0-9].[0-9]{2}")

[[ -d ${PREFIX}/bin/ghost ]] && rm ${PREFIX}/bin/ghost

[[ -d ${PREFIX}/lib/python$v/site-packages/ghost ]] && {
	rm -rf ${PREFIX}/lib/python$v/site-packages/ghost;
}

[[ -d ${PREFIX}/lib/python$v/site-packages/ghost-8.0.0.dist-info ]] && {
  rm -rf ${PREFIX}/lib/python$v/site-packages/ghost-8.0.0.dist-info
}

python -m pip --no-color --quiet --no-cache-dir uninstall adb-shell

####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
