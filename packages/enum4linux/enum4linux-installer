#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Downloading executable ..."
wget --tries=20 --quiet https://raw.githubusercontent.com/CiscoCXSecurity/enum4linux/master/enum4linux.pl \
  -O ${PREFIX}/bin/enum4linux
chmod +x ${PREFIX}/bin/enum4linux
echo "Installing dependencies(It could take a while) ..."
for dep in WWW::Mechanize Net::IP Net::DNS Net::Netmask XML::Writer String::Random; do
  cpan -i $dep
done
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
