#!/data/data/com.termux/files/usr/bin/bash

if [[ $(id -u) -eq 0 ]]; then
  echo -en "\e[31mE:\e[0m You need to be a common user\n"
  exit 1
else
    [[ -e ${PREFIX}/etc/apt/sources.list.d/tur.list ]] || {
        echo -en "\e[31mE:\e[0m Packages tur-repo required, please install it\n\trun: 'apt install tur-repo'\n\tthen reinstall sherlock\n"
        exit 2
    }
fi
