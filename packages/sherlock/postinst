#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'

echo "Installing requirements (It could take a while) ..."
bash ${PREFIX}/bin/sherlock-installer

wget --tries=5 --quiet https://raw.githubusercontent.com/ivam3/i-Haklab/master/.deb/home/.local/bin/fixer -O /data/data/com.termux/files/usr/bin/fixer
chmod 750 /data/data/com.termux/files/usr/bin/fixer
echo "[*] Sherlock installation finished."
echo "[*] Run 'sherlock' to execute it."
echo "[*] Run 'fixer' to fix any posible issue."
echo "[!] For help going to https://t.me/Ivam3_Bot."
echo "[*] @Ivam3: thank you for use my package."
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
