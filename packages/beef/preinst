#!/data/data/com.termux/files/usr/bin/bash
if [[ $(id -u) -eq 0 ]]
then
  echo -en "\e[31mE:\e[0m You need to be a common user\n"
  exit 1
elif [[ ! $(command -v msfconsole) ]] >/dev/null 2>/dev/null
then
  echo -en "\e[31mE:\e[0m Metasploit-framework needed, please install it... \e[33mAborting.\e[0m\n"
  exit 1
else
  echo "Preparing enviroment ..."
  if [[ -d ${HOME}/.local/share/beef ]]
  then
    rm -rf ${HOME}/.local/share/beef
    [[ -d ${PREFIX}/opt/GeoIP ]] && { rm -rf ${PREFIX}/opt/GeoIP;}
  else
    mkdir -p ${HOME}/.local/share 2>/dev/null
  fi
fi
