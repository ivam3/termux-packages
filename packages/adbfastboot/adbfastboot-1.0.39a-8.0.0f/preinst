#!/data/data/com.termux/files/usr/bin/bash
if [[ $(id -u) -eq 0 ]]
then
	echo -en "\e[31m(_âž¤)\e[0m You need to be a common user"
	exit 1
else
	echo "Preparing enviroment ..."
	if [[ -d ${HOME}/.local/share/adbfastboot ]]
	then
		rm -rf ${HOME}/.local/share/adbfastboot
	else
		mkdir -p ${HOME}/.local/share 2>/dev/null
	fi
fi

