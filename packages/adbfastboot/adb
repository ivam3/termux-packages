#!/data/data/com.termux/files/usr/bin/bash
path="$(cd "$(dirname "$0")"; pwd)";
if [[ -d ${HOME}/.local/share/adbfastboot ]]
then
	mkdir -p ${HOME}/.local/share/adbfastboot
fi
export HOME=/data/data/com.termux/files/home/.local/share/adbfastboot;
export TMPDIR=/data/data/com.termux/files/usr/tmp;
$dir/adb.bin "$@";
