#!/data/data/com.termux/files/usr/bin/bash
# COLORS
W="\e[0m"
R="\e[31m"
G="\e[32m"
Y="\e[33m"
B="\e[34m"
if [[ -d ${HOME}/.local/share/johntheripper ]]
then
  cd ${HOME}/.local/share/johntheripper >/dev/null
	./$(basename $0) $@
else
  echo -en $R"E:$W $(basename $0) the ripper PATH missing ...$Y Aborting.$W"
  exit 1
fi
