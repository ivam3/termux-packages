#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Downloading executable ..."
wget --tries=20 --quiet https://raw.githubusercontent.com/miluxmil/milux/master/converter \
  -O ${PREFIX}/bin/converter
[[ -e ${PREFIX}/bin/converter ]] && {
  chmod 751 ${PREFIX}/bin/converter;
} || {
  echo -en "\e[31mE:\e[0m converter executable missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
