#!/data/data/com.termux/files/usr/bin/bash

echo "Cloning repository ..."
git clone --quiet https://github.com/threat9/routersploit ${HOME}/.local/share/routersploit
chmod 750 -R ${HOME}/.local/share/routersploit
echo "Installing requirements ..."
python -m pip --no-color --quiet --no-cache-dir install \
	future requests pysnmp pycrypto cryptography paramiko bcrypt pynacl
exit 0
