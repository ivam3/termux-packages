#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Downloading zip file (It could take a while) ..."
wget --tries=20 -qc https://github.com/danielmiessler/SecLists/archive/master.zip \
  -O ${HOME}/.local/share/wordlist/SecLists.zip
if [[ -e ${HOME}/.local/share/wordlist/SecLists.zip ]] && [[ -s ${HOME}/.local/share/wordlist/SecLists.zip ]]
then
  echo "Unzipping files ..."
  unzip -q ${HOME}/.local/share/wordlist/SecLists.zip -d ${HOME}/.local/share/wordlist/
  wget --tries=20 --quiet https://www.scrapmaker.com/data/wordlists/dictionaries/rockyou.txt \
    -O ${HOME}/.local/share/wordlist/rockyou.txt;
  rm -rf ${HOME}/.local/share/wordlist/SecLists.zip
  echo "[*] seclists installation finished."
  echo "[*] seclists was saved in \$HOME/.local/share/wordlist."
else
  echo -en "\e[31mE:\e[0m seclists zip file missing ... \e[33mAborting.\e[0m\n"
fi
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
