#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet https://github.com/JasonJerry/lockphish ${HOME}/.local/share/lockphish
[[ -d ${HOME}/.local/share/lockphish ]] && {
  echo "Installing requirements ...";
  chmod 751 -R ${HOME}/.local/share/lockphish;
  ln -s $(command -v ngrok) ${HOME}/.local/share/lockphish/ngrok
} || {
  echo -en "\e[31mE:\e[0m lockphish directory missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
