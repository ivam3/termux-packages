#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Downloading executable ..."
wget --tries=20 --quiet https://raw.githubusercontent.com/yanncam/UnSHc/master/latest/unshc.sh \
  -O ${PREFIX}/bin/unshc
[[ -e ${PREFIX}/bin/unshc ]] && {
  sed 's|which|command -v|g' -i ${PREFIX}/bin/unshc;
  sed 's|/tmp/XXXXXX|$PREFIX/tmp/XXXXXX|g' -i ${PREFIX}/bin/unshc;
  chmod +x ${PREFIX}/bin/unshc;
} || {
  echo -en "\e[31mE:\e[0m unshc missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
