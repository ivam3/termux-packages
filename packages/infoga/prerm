#!/data/data/com.termux/files/usr/bin/bash
if [[ -d ${HOME}/.local/share/infoga ]]; then
	pushd ${HOME}/.local/share/infoga
	python -m pip --no-color --quiet --no-cache-dir uninstall -r requirements.txt
	popd
  rm -rf ${HOME}/.local/share/infoga
fi
