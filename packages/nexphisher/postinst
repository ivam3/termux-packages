#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
#wget --tries=20 --quiet \
# https://raw.githubusercontent.com/htr-tech/nexphisher/master/nexphisher \
#	-O ${HOME}/.local/share/nexphisher/nexphisher
#if [[ -e ${HOME}/.local/share/nexphisher/nexphisher ]]
#then
#  ln=$(grep -n "ngrok_link=\$(curl" ${HOME}/.local/share/nexphisher/nexphisher|awk -F ":" '{print $1}')
#  #sed -i "$ln {s/^/#/}" ${HOME}/.local/share/nexphisher/nexphisher
#  #sed -i "${ln}a ngrok_link=\$(curl -s -N http://127.0.0.1:4040/api/tunnels|cut -d '\"' -f18)" \
#  #  ${HOME}/.local/share/nexphisher/nexphisher
#else
#	echo -en "\e[31mE:\e[0m Executable missing.\n"
#fi
  
curl -fsSL https://raw.githubusercontent.com/htr-tech/nexphisher/master/Modules.zip \
	-o ${TMPDIR}/Modules.zip

if [[ -e ${TMPDIR}/Modules.zip ]]
then
	unzip -qq ${TMPDIR}/Modules.zip -d ${HOME}/.local/share/nexphisher
	rm ${TMPDIR}/Modules.zip
else
	echo -en "\e[31mE:\e[0m Modules missing.\n"
fi

[[ ${PREFIX}/bin/ngrok ]] && [[ -e ${HOME}/.local/share/nexphisher/.htr/ngrok ]] || {
	ln -s ${PREFIX}/bin/ngrok ${HOME}/.local/share/nexphisher/.htr/ngrok
}

[[ -e ${HOME}/.local/share/nexphisher/.htr/loclx ]] || {
	arch1=$(uname -a|grep -o 'arm'|head -n1)
	arch2=$(uname -a|grep -o 'Android'|head -n1)
	[[ $arch1 = 'arm' ]] || [[ $arch2 = 'Android' ]] && arch=arm || arch=386
	curl -sLO https://lxpdownloads.sgp1.digitaloceanspaces.com/cli/loclx-linux-$arch.zip \
		-o ${TMPDIR}/loclx-linux-$arch.zip
	[[ -e ${TMPDIR}/loclx-linux-$arch.zip ]] && {
		unzip -qq loclx-linux-$arch.zip -d ${HOME}/.local/share/nexphisher/.htr;
		rm -rf ${TMPDIR}/loclx-linux-$arch.zip;
		chmod +x ${HOME}/.local/share/nexphisher/.htr/loclx;
	}
}

[[ -d ${HOME}/.ssh ]] || { mkdir ${HOME}/.ssh;touch ${HOME}/.ssh/known_hosts;}
printf "|1|SVjtt9mtL4P/5lsqaielF1pJHvM=|6QmJXxfPd9A5uUaFI1RV2H4brTs= ssh-rsa \n|1|PiraXMKgzzt9DdPRSLAtRvRad0Y=|2W162QXfHJvKKZWIaRyZO6VFWJI= ssh-rsa \n" >> ${HOME}/.ssh/known_hosts
exit 0
curl -fsSL https://raw.githubusercontent.com/ivam3/i-Haklab/master/.deb/home/.local/bin/fixer 
-o /data/data/com.termux/files/usr/bin/fixer
chmod 751 /data/data/com.termux/files/usr/bin/fixer
echo "[*] Nexphisher installation finished."
echo "[*] Run 'nexphisher' to execute it."
echo "[*] Run 'fixer' to fix any posible issue."
echo "[!] For help going to https://t.me/Ivam3_Bot."
echo "[*] @Ivam3: thank you for use my package."
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
