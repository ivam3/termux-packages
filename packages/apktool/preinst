#!/data/data/com.termux/files/usr/bin/bash
if [[ $(id -u) -eq 0 ]]
then
  echo -en "\e[31mE:\e[0m You need to be a common user\n"
  exit 1
else
  echo "Preparing enviroment ..."
  if [[ -e ${PREFIX}/var/spool/apktool.jar ]]
  then
    rm -rf ${PREFIX}/var/spool/apktool.jar
  else
    mkdir -p ${PREFIX}/var/spool/ 2>/dev/null
  fi
fi
