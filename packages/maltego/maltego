#!/data/data/com.termux/files/usr/bin/bash
# COLORS
W="\e[0m"
R="\e[31m"
G="\e[32m"
Y="\e[33m"
B="\e[34m"
if [[ -d ${HOME}/.local/share/maltego/bin ]]
then
  export JAVA_HOME=${PREFIX}/opt/openjdk-21
  cd ${HOME}/.local/share/$(basename $0)/bin >/dev/null
  [[ $(file $TMPDIR/*|grep -o "socket") >/dev/null ]] || {
    printf >&2 "$R(âž¤_)\$W Termux:Wayland server isn't up...$R aborting.$W"
    exit 1
  }
  export DISPLAY=:0
  $(command -v sh) $(basename $0) ${@:1}
else
  echo -en $R"E:$W $(basename $0) PATH missing ...$Y Aborting.$W"
  exit 1
fi
