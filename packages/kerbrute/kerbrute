#!/data/data/com.termux/files/usr/bin/bash
# COLORS
W="\e[0m"
R="\e[31m"
G="\e[32m"
Y="\e[33m"
B="\e[34m"

case `dpkg --print-architecture` in 
  aarch64|amd64|x86_64) 
    arch="amd64" ;;
  arm|i*86|x86)
    arch="386" ;;
  *)
    echo "unknown architecture"; exit 1 ;;
esac

if [[ -d ${HOME}/.local/share/kerbrute/dist ]]
then
  cd ${HOME}/.local/share/$(basename $0)/dist >/dev/null
  ./$(basename $0)_linux_$arch ${@:1}
else
  echo -en $R"E:$W $(basename $0) PATH missing ...$Y Aborting.$W"
  exit 1
fi
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
