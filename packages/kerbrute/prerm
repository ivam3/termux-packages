#!/data/data/com.termux/files/usr/bin/bash
if [[ -d ${HOME}/.local/share/kerbrute ]]; then
  cd ${HOME}/.local/share/kerbrute
  python2 -m pip --no-color --quiet --no-cache-dir uninstall -r requirements.txt
  rm -rf ${HOME}/.local/share/kerbrute
fi
