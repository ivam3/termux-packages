#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet --depth 1 https://github.com/fuzzdb-project/fuzzdb ${HOME}/.local/share/fuzzdb
chmod 750 -R ${HOME}/.local/share/fuzzdb
echo "Installing binary ..."
python -m pip --no-color --quiet --no-cache-dir install wfuzz
exit 0
