#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Downloading executable ..."
wget --tries=20 --quiet https://raw.githubusercontent.com/cddmp/enum4linux-ng/master/enum4linux-ng.py \
  -O ${PREFIX}/bin/enum4linux-ng
chmod +x ${PREFIX}/bin/enum4linux-ng
echo "Installing dependencies(It could take a while) ..."
for m in ldap3 PyYAML impacket; do
  python -m pip --no-color --quiet --no-cache-dir install $m
done
exit 0
