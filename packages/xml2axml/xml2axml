#!/data/data/com.termux/files/usr/bin/bash
# COLORS
W="\e[0m"
R="\e[31m"
G="\e[32m"
Y="\e[33m"
B="\e[34m"
if [[ -e ${HOME}/.local/share/xml2axml.jar ]]
then
	umask 000
	exec $(command -v java) \
        --add-opens=java.desktop/javax.swing=ALL-UNNAMED \
        --add-opens=java.base/java.lang=ALL-UNNAMED \
        -jar ${HOME}/.local/share/xml2axml.jar
else
	echo -en "$R(âž¤_)$W Jar file is not installed...$R aborting.$W"
	exit 1
fi
