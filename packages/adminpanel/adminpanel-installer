#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet https://github.com/Techzindia/admin_penal ${HOME}/.local/share/adminpanel
[[ -d ${HOME}/.local/share/adminpanel ]] && {
  echo "Installing requirements ...";
  chmod 751 -R ${HOME}/.local/share/adminpanel;
  cd ${HOME}/.local/share/adminpanel;
  mv admin_panel_finder.py adminpanel.py;
} || {
  echo -en "\e[31mE:\e[0m adminpanel directory missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
