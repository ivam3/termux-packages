#!/data/data/com.termux/files/usr/bin/bash
maindir=$(df|grep fuse|awk -F "/" '{print $NF}'|grep -iv emulated)
rm -rf ${HOME}/Desktop 2>/dev/null
rm ${HOME}/{Documents,downloads,Pictures,Public,Video} 2>/dev/null
rm -rf $maindir/{Documents,downloads,Pictures,Public,Video} 2>/dev/null
