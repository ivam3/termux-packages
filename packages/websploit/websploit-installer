#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet https://github.com/The404Hacking/websploit ${HOME}/.local/share/websploit
[[ -d ${HOME}/.local/share/websploit ]] && {
  echo "Installing requirements ...";
  chmod 751 -R ${HOME}/.local/share/websploit;
  python2 -m pip --no-color --quiet --no-cache-dir install numpy;
} || {
  echo -en "\e[31mE:\e[0m websploit directory missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
