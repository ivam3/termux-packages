#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet https://github.com/qashqao/acccheck ${HOME}/.local/share/acccheck
[[ -d ${HOME}/.local/share/acccheck ]] && {
  chmod 751 -R ${HOME}/.local/share/acccheck;
} || {
  echo -en "\e[31mE:\e[0m acccheck directory missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
