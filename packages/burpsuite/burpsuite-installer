#!/data/data/com.termux/files/usr/bin/bash

[[ ! -e $PREFIX/opt/openjdk/lib/libgif.so.7 ]] && [[ -e $PRERIX/lib/libgif.so.7.2.0 ]] && {
	ln -s $PRERIX/lib/libgif.so.7.2.0 $PREFIX/opt/openjdk/lib/libgif.so.7;
}
echo "Downloading jar file (It could take a while) ..."
wget -q --tries=20 https://portswigger.net/burp/releases/download \
	-O ${HOME}/.local/share/burpsuite.jar
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
