#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Downloading executable ..."
wget --tries=20 --quiet https://gitlab.com/kalilinux/packages/hash-identifier/-/raw/kali/master/hash-id.py \
  -O ${PREFIX}/bin/hashid
[[ -e ${PREFIX}/bin/hashid ]] && {
  echo "Giving permissions ...";
  chmod 751 -R ${PREFIX}/bin/hashid; 
} || {
  echo -en "\e[31mE:\e[0m Missing hashid executable ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
####::: REPORT ISSUES AT https://t.me/Ivam3_Bot :::####
