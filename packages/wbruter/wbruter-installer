#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet https://github.com/wuseman/WBRUTER ${HOME}/.local/share/wbruter
[[ -d ${HOME}/.local/share/wbruter ]] && {
  echo "Installing requirements ...";
  chmod 751 -R ${HOME}/.local/share/wbruter;
} || {
  echo -en "\e[31mE:\e[0m wbruter directory missing ... \e[33mAborting.\e[0m\n";
  exit 1;
}
exit 0
