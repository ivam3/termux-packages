#!/data/data/com.termux/files/usr/bin/bash
IFS=$'\n\t'
echo "Cloning repository ..."
git clone --quiet https://github.com/b3-v3r/Hunner ${HOME}/.local/share/hunner-framework
chmod 750 -R ${HOME}/.local/share/hunner-framework
echo "Installing dependencies(It could take a while) ..."
python -m pip --no-color --quiet --no-cache-dir install httplib2
exit 0
