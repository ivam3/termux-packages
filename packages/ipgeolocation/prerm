#!/data/data/com.termux/files/usr/bin/bash
if [[ -d ${HOME}/.local/share/ipgeolocation ]]; then
	pushd ${HOME}/.local/share/ipgeolocation
	python -m pip --no-color --quiet --no-cache-dir uninstall -r requirements.txt
  popd
	rm -rf ${HOME}/.local/share/ipgeolocation
fi
